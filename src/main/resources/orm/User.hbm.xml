<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="by.sam.horbach.ticketService.entities.User"
		table="user" catalog="ticketService">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="email" type="string">
			<column name="email" length="45" not-null="true" />
		</property>
		<property name="password" type="string">
			<column name="password" length="20" not-null="true" />
		</property>
		<property name="enabled">
			<column name="enabled" not-null="true" />
		</property>
		<property name="name" type="string">
			<column name="name" length="45" />
		</property>
		<property name="surname" type="string">
			<column name="surname" length="45" />
		</property>
		<property name="role">
			<column name="Role_id" />
			<type
				name="converted::by.sam.horbach.ticketService.converters.UserRolesConverter" />
		</property>
		<property name="iconPath">
			<column name="iconPath" />
			<type
				name="converted::by.sam.horbach.ticketService.converters.PathConverter" />
		</property>
	</class>
</hibernate-mapping>